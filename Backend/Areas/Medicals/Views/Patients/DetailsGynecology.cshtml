    </section>
    <div>
        <a class="btn btn-primary" href="@Url.Action("CreateGynecologyVisits", "Patients", new { area = "Medicals", id = @Model.GynecologyId })"> <span class="fa fa-plus"></span>   Registrar Visita</a>

    </div>
    <br />
    <div class="row">
        <div class="col-md-12">

            @if (Model.GynecologyVisits.Count == 0)
            {
            <h2>No existen Visitas registradas aun :'(</h2>
            }
            else
            {
            <div class="scroller">
                <table class="table">
                    <thead>
                        <tr><th></th>
                            <th>
                                Fecha
                            </th>
                            <th>
                                Razon de la Visita
                            </th>
                            <th>
                                Indicaciones
                            </th>
                          
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.GynecologyVisits)
                        {
                        <tr>
                            <td>
                                <a class="btn btn-warning" href="@Url.Action("EditGynecologicalVisits", "Patients", new { area = "Medicals", id = @item.GynecologyVisitId })"> <span class="fa fa-pencil-square-o "></span> Editar</a>
                                <a class="btn btn-danger" href="@Url.Action("DeleteGynecologyVisits", "Patients", new { area = "Medicals", id = @item.GynecologyVisitId })" onclick="return confirm('Esta seguro que desea eliminar este registro?')"> <span class="fa fa-trash-o"></span> Eliminar </a>
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.VisitDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.VisitReason)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Indications)
                            </td>
                          
                        </tr>
                        }
                    </tbody>
                </table>
            </div>
            }
        </div>

    </div>
