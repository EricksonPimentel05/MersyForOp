@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Areas/Medicals/JS/AnaliticalSheet.js"></script>
    <script type="text/javascript">

        $(document).ready(function () {

            
            $("#BtnPrintA4Empty").click(function () {

                var url = '@Url.Action("AnaliticalSheetA4Empty", "Patients",new {area="Medicals"})';

                url += "/" + $("#AnalyticalSheetId").val();

                var win = window.open(url);

                if (win) {
                    //Browser has allowed it to be opened
                    win.focus();
                } else {
                    //Browser has blocked it
                    alert("Porfavor, debes permitir que se abran las ventanas emergentes o el reporte no va a salir :'( ");
                }
                // window.location.href = url;
            });
            $("#BtnPrintA4").click(function () {

                var url = '@Url.Action("AnaliticalSheetA4", "Patients",new {area="Medicals"})';

                url += "/" + $("#AnalyticalSheetId").val();

                var win = window.open(url);

                if (win) {
                    //Browser has allowed it to be opened
                    win.focus();
                } else {
                    //Browser has blocked it
                    alert("Porfavor, debes permitir que se abran las ventanas emergentes o el reporte no va a salir :'( ");
                }
                // window.location.href = url;
            });


        });
    </script>
}
