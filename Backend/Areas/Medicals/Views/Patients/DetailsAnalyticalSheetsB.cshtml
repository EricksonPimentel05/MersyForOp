@model Domain.MED.AnalyticalSheet
@{
    ViewBag.Title = "Detalle de Analitica";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="form-horizontal">
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.HiddenFor(model => model.PatientId)
    @Html.HiddenFor(model => model.AnalyticalSheetId)
    @Html.HiddenFor(model => model.DoctorText)
    @Html.HiddenFor(model => model.PatientName)
    <p>
        <a class="btn btn-success" href="@Url.Action("IndexAnalyticalSheetsB", "Patients", new { area = "Medicals", id = @Model.PatientId })">
            <span class="fa fa-reply"></span> Volver atras
        </a>
        <a class="btn btn-warning" href="@Url.Action("EditAnalyticalSheetsB", "Patients", new { area = "Medicals", id = @Model.AnalyticalSheetId })"> <span class="fa fa-pencil-square-o"></span> Editar Expediente </a>
        @*<a class="btn btn-primary" id="BtnPrint">
            <span class="fa fa-print"></span> Imprimir
        </a>*@

        <a class="btn btn-primary" id="BtnPrintA4">
            <span class="fa fa-print"></span> Imprimir en A4
        </a>
        <a class="btn btn-primary" id="BtnPrintA4Empty">
            <span class="fa fa-print"></span> Imprimir Sin Formato en A4
        </a>
        @*<a class="btn btn-primary" id="BtnPrintA6">
            <span class="fa fa-print"></span> Imprimir Media Pagina
        </a>*@
    </p>
    <div class="form-group">
        @Html.LabelFor(model => model.AnalyticalSheetDate, new { @class = "control-label col-md-2" })
        <div class="col-md-2">
            @Html.EditorFor(model => model.AnalyticalSheetDate, new { htmlAttributes = new { @class = "form-control PsDates" } })
            @Html.ValidationMessageFor(model => model.AnalyticalSheetDate, "", new { @class = "text-danger" })
        </div>
    </div>
