function getBursh(n){for(var t=0;t<brushes.length;t++)if(brushes[t].indexOf(n)>-1&&brushes[t].length-2!=brushes[t].indexOf(n))return brushes[t]}function isFlashEnabled(){var n=!1,t;try{t=new ActiveXObject("ShockwaveFlash.ShockwaveFlash");t&&(n=!0)}catch(i){navigator.mimeTypes["application/x-shockwave-flash"]!=undefined&&(n=!0)}return n}function loadBursh(){var t=$("*[class^='brush:']"),n=[];return t.each(function(t,i){n.push(getBursh(i.className.split(":")[1]))}),canstop=setInterval(function(){if($(".codesnippet").find(".syntaxhighlighter").length==$(".codesnippet").length){var n=$(".codesnippet .syntaxhighlighter");n.each(function(n,t){if(isFlashEnabled()){$(t).parents("div.codesnippet").prev().append('<button class="copytoclipboard" id="copyButton_'+t.id+'" title="Copy to Clipboard" data-clipboard-target="CopyCode_'+t.id+'"><\/button> ');var i=new ZeroClipboard(document.getElementById("copyButton_"+t.id),{moviePath:"/Content/resources/ZeroClipboard.swf"});quickCodeHandler($("#"+t.id),t.id)}n==$(".codesnippet").length-1&&shLineWrap()});clearInterval(canstop)}},3e3),n}function path(){for(var t=arguments[0],i=[],n=0;n<t.length;n++)i.push(t[n].replace("$","/Scripts/components/others/").trim());return i}function quickCodeHandler(n,t){var o=n.parent(),e=$(o).find(".container"),u=document.createElement("textarea"),f,i,r;for(u.id="CopyCode_"+t,u.style.display="none",f=e.children(),i=[],r=0;r<f.length;r++)i.push(f[r].innerText||f[r].textContent);i=i.join("\r");u.appendChild(document.createTextNode(i));e.append(u)}function beforeLoadMore(){$("#loadmore").showWaitingPopUp({opacityRange:0})}function loadMoreOnSuccess(){$("#loadMoreComments>table >tbody>tr:last-child>td").css("border-bottom","0px");parseInt($("#totalCommentCount").val())-10<10&&$("#count").text(parseInt($("#totalCommentCount").val())-10);removeWaitingPopupFromElement("loadmore");$("#startIndex").val($("#commentscount").val());$("#articleId").val($("#article-id").val());$("#isLastComment").val()=="True"&&$("#loadmore").hide()}function loadMoreOnFailure(){removeWaitingPopupFromElement("loadmore");window.location=window.location.href}function ajaxLoginFn(){if($("#KBEmail").val()!=""&&$("#KBPassword").val()!=""){var n='{"username":"'+$("#KBEmail").val()+'", "password":"'+$("#KBPassword").val()+'", "rememberMe":"'+$("#KBRememberme").is(":checked")+'"}';$.ajax({type:"POST",url:"/account/ajaxlogin",beforeSend:$("#KBloginSection").showWaitingPopUp(),data:n,contentType:"application/json; charset=utf-8",dataType:"json",success:onLoginSuccess,error:function(){$("#KBloginSection").removeWaitingPopUp();window.location=window.location.href}})}else $("#KBEmail").val()==""&&$("#KBPassword").val()==""?($("#KBEmailValidationError").html("Please enter your email address").show(),$("#KBPasswordValidationError").html("Please enter your password").show()):$("#KBEmail").val()==""?($("#KBEmailValidationError").html("Please enter your email address").show(),$("#KBPasswordValidationError").hide()):($("#KBEmailValidationError").hide(),$("#KBPasswordValidationError").html("Please enter your password").show())}function onLoginSuccess(n){$("#KBloginSection").removeWaitingPopUp();n!=null&&(n.IsLoginSuccess?($("#menu-signin").html('<a href="/account/logoff" class="login-button default-btn">SIGN OUT<\/a>'),$("#menu-signin").css("background-color","#050707"),click=!1,$("#login-section").slideUp("slow"),$("#isAuthenticated").val("True"),$("#addCommentSection").load("/kb/ShowPostCommentSection",function(n,t){rteRender();t=="success"&&($("#articleID").val($("#article-id").val()),$("#KBEmail").val($("#KBEmail").val()))})):n.EmailResult!=null&&n.EmailResult!=""&&n.PasswordResult!=null&&n.PasswordResult!=""?($("#KBEmailValidationError").html(n.EmailResult).show(),$("#KBPasswordValidationError").html(n.PasswordResult).show()):n.EmailResult!=null&&n.EmailResult!=""?($("#KBEmailValidationError").html(n.EmailResult).show(),$("#KBPasswordValidationError").hide()):n.PasswordResult!=null&&n.PasswordResult!=""&&($("#KBEmailValidationError").hide(),$("#KBPasswordValidationError").html(n.PasswordResult).show()))}function LogVote(n){var t={};t.articleId=$("#article-id").val();t.isHelpful=n;!readCookie("hasVoted")&&canSubmitVote&&($("#voteInfo").html("Thank you for your feedback").stop(!0,!0).fadeIn("fast").delay(3e3).fadeOut("slow"),$("#isHelpFull").delay(3e3).fadeOut("slow"),canSubmitVote=!1,$.ajax({type:"get",url:"/Support/kb/logvote",data:t,contentType:"application/json; charset=utf-8",dataType:"json",success:function(n){n.IsSuccess&&(createCookie("hasVoted",n.IsSuccess,365),canSubmitVote=!0)},error:function(){canSubmitVote=!0;window.location=window.location.href}}))}function createCookie(n,t,i){var r,u;i?(r=new Date,r.setTime(r.getTime()+i*864e5),u="; expires="+r.toGMTString()):u="";document.cookie=n+"="+t+u+";"}function readCookie(n){for(var r=n+"=",u=document.cookie.split(";"),t,i=0;i<u.length;i++){for(t=u[i];t.charAt(0)==" ";)t=t.substring(1,t.length);if(t.indexOf(r)==0)return t.substring(r.length,t.length)}return!1}function rteRender(){$("#comment").ejRTE({width:"98%",height:"200px",minWidth:"150px",showFooter:!1,showHtmlSource:!0,showContextMenu:!1,iframeAttributes:{spellcheck:"true"},tools:{edit:["findAndReplace"],style:["bold","italic","underline","strikethrough"],alignment:["justifyLeft","justifyCenter","justifyRight","justifyFull"],lists:["unorderedList","orderedList"],clipboard:["cut","copy","paste"],doAction:["undo","redo"],indenting:["outdent","indent"],clear:["clearFormat","clearAll"],images:["image"],media:["video"],tables:["createTable","addRowAbove","addRowBelow","addColumnLeft","addColumnRight","deleteRow","deleteColumn","deleteTable"],effects:["superscript","subscript"],casing:["upperCase","lowerCase"],customUnorderedList:[{name:"unOrderInsert",tooltip:"Custom UnOrderList",css:"e-rte-toolbar-icon e-rte-unlistitems customUnOrder",text:"Smiley",listImage:"url('../content/images/rte/Smiley-GIF.gif')"}],customOrderedList:[{name:"orderInsert",tooltip:"Custom OrderL