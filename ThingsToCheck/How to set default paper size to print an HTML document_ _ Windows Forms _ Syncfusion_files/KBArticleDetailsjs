n=$(this).parents("#reply-comment").find("#rte-"+t).ejRTE("getHtml");if(n=n.replace("<br>","").replace(" ","").replace("&nbsp;","").replace("<div>","").replace("<\/div>",""),n==""||n==null||n.trim().length==0)return $(this).parents("#reply-comment").find("#ReplyValidation").html("Please enter comment"),!1;$("#reply-comment-form").submit()});$(document).on("click","#submitComment",function(){var n=$("#comment").ejRTE("getText");if(n=n.replace("<br>","").replace(" ","").replace("&nbsp;","").replace("<div>","").replace("<\/div>",""),n==""||n==null||n.trim().length==0)return $("#CommentValidation").html("Please enter comment"),!1;$("#addComment").submit()});$(document).on("click",".reply-button",function(){if($("#isAuthenticated").val()=="False")$("html, body").animate({scrollTop:$("#KBloginSection").offset().top},200);else{commentId==0?commentId=$(this).siblings("#reply-to-id").val():$("#"+commentId).siblings("#reply-comment").hide();$(this).siblings("#reply-comment").find("#email").val($("#addCommentSection").find("#email").val());$(this).siblings("#reply-comment").find("#article-ID-reply").val($("#article-id").val());var n=$(this).siblings("#reply-to-id").val();$(this).siblings("#reply-comment").find($("#rte-"+n)).ejRTE({width:"100%",height:"200px",name:"comment",showFooter:!1,showHtmlSource:!0,showContextMenu:!1,iframeAttributes:{spellcheck:"true"},tools:{edit:["findAndReplace"],style:["bold","italic","underline","strikethrough"],alignment:["justifyLeft","justifyCenter","justifyRight","justifyFull"],lists:["unorderedList","orderedList"],clipboard:["cut","copy","paste"],doAction:["undo","redo"],indenting:["outdent","indent"],clear:["clearFormat","clearAll"],images:["image"],media:["video"],tables:["createTable","addRowAbove","addRowBelow","addColumnLeft","addColumnRight","deleteRow","deleteColumn","deleteTable"],effects:["superscript","subscript"],casing:["upperCase","lowerCase"],customUnorderedList:[{name:"unOrderInsert",tooltip:"Custom UnOrderList",css:"e-rte-toolb