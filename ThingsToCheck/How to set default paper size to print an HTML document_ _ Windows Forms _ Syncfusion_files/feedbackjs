$(window).load(function(){var n=window.location.href;if(n.toLowerCase().indexOf("support/directtrac/incidents")<0&&n.toLowerCase().indexOf("support/directtrac/sampleincidents")<0&&n.toLowerCase().indexOf("support/feedback/")<0){$("#feedbackButton").removeClass("hide");var i=$("#feedbackButton").outerHeight(),r=$(window).height(),u=(r-i)/2,t=$(window).width();t>1300&&t<1366?$("#feedbackButton").css("top","240px"):$("#feedbackButton").css("top",u)}});$("#feedbacks-dialogue").ejDialog({width:"0px",height:"470px",showHeader:!1,draggable:!1,enableResize:!1,enableModal:!1,closeOnEscape:!1,showOnInit:!1});$("#feedbackButton").on("click",function(){var t=480,i=343,u=$(window).width(),n;$("#feedbacks-dialogue").ejDialog("open");$("#feedbacks-dialogue").hide();u>1820&&(t=500,i=350);n=$(".form-control").prop("style");n.removeProperty("color");n=$("#feedbacks-dialogue_wrapper").prop("style");n.removeProperty("left");n=$("#feedbacks-dialogue_wrapper").prop("style");n.removeProperty("top");var f=t,e=$(window).height(),r=(e-t)/2;$("#feedbacks-dialogue_wrapper").css("top",r);$("#feedbacks-dialogue_wrapper").attr("style","height:"+f+"px !important;top:"+r+"px;width:0px;");$("#feedbacks-dialogue_wrapper").animate({width:i+"px"},600);$("#feedbacks-dialogue").show(500);$("#feedbacks-dialogue_wrapper").showWaitingPopUp();$("#feedbacks-dialogue").addClass("hide");$(".e-dialog-scroller").css("height",t);$.ajax({type:"GET",url:"/account/GetFeedbackCategory",cache:!1,success:function(n){$("#feedbacks-dialogue").html(n);$("#feedbacks-dialogue").removeClass("hide");$("#feedbacks-dialogue_wrapper").removeWaitingPopUp();$("#feedbackform").click();$("#name").focus();$("#name").select();$("#login-btn-menu").length!=1&&($("#feedbackform").click(),$("#message").focus(),$("#message").select())}})})